name: motherfucker
on:
  push:
    branches:
      - main

jobs:
  sync_with_upstream:
    runs-on: ubuntu-latest
    name: auto checkin for traffic

    steps:
      - name: checkout
        uses: actions/checkout@v3
        with:
          ref: master

      - name: checkin
        uses: actions/setup-python@v2
        with:
          python-version: "3.x"
          architecture: "x64"
      - run: python ./auto-checkin.py

      - name: timestamp
        run: date
